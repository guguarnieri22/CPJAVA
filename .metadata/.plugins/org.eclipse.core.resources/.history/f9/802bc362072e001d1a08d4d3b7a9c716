package entrada_saida_processamento;

import java.util.Scanner;

public class Ex24 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		
		String nome, sexo, estado_civil;
		int anos;
		
		System.out.printf("digite seu nome: ");
		nome = ler.next();
		System.out.printf("digite seu sexo em f: ");
		sexo = ler.next();
		System.out.printf("digite seu estado civil: ");
		estado_civil = ler.next();
		
		if (sexo = "f") && (estado_civil = "casada"){
			System.out.printf("digite o tempo de casado");
			anos = ler.nextInt();
		}
	}

}
